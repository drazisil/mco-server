{
  "name": "mco-server",
  "version": "3.0.0-beta1",
  "description": "a server",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "AUTHORS"
  ],
  "scripts": {
    "start": "node app.js",
    "test": "npm run lint-test",
    "migrate": "pg-migrator postgres://postgres:mco@localhost/postgres 2",
    "lint": "standard --fix src/**/*.js",
    "lint-test": "standard --fix src/**/*.js && nyc mocha",
    "precommit": "npm run lint-test"
  },
  "author": "Joe Becher <jwbecher@drazisil.com> (drazisil.com)",
  "license": "MPL-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:drazisil/mco-server.git"
  },
  "dependencies": {
    "c-struct": "0.0.5",
    "ini": "2.0.0",
    "pg": "8.6.0",
    "pg-migrator": "1.1.0",
    "postgres-migrations": "5.1.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/c-struct": "0.0.0",
    "@types/chai": "4.2.18",
    "@types/ini": "1.3.30",
    "@types/mocha": "8.2.2",
    "@types/mock-fs": "4.13.0",
    "@types/node": "14.14.44",
    "@types/pg": "7.14.11",
    "@types/sinon": "10.0.0",
    "@types/supertest": "2.0.11",
    "@types/verror": "1.10.4",
    "chai": "4.3.4",
    "mocha": "8.4.0",
    "mock-fs": "4.14.0",
    "nyc": "15.1.0",
    "sinon": "10.0.0",
    "source-map-support": "0.5.19",
    "standard": "16.0.3",
    "supertest": "6.1.3",
    "testdouble": "3.16.1"
  },
  "nyc": {
    "all": true,
    "useSpawnWrap": true,
    "reporter": [
      "text",
      "text-summary",
      "html",
      "cobertura"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "out/*",
      "coverage/*",
      "test/*",
      "dist/*",
      "*/**/*.test.{ts,js}"
    ]
  }
}
