{
  "name": "mco-server",
  "version": "3.0.0-beta1",
  "description": "a server",
  "main": "src/server.js",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "AUTHORS"
  ],
  "scripts": {
    "start": "node src/app.js",
    "test": "npm run lint-test",
    "doc": "jsdoc -R README.md -P package.json -r src",
    "migrate": "pg-migrator postgres://postgres:mco@localhost/postgres 2",
    "prod": "node src/app.js | ./node_modules/.bin/bunyan",
    "lint-test": "prettier-standard --format && tap --coverage test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "precommit": "prettier-standard --lint"
  },
  "author": "Joe Becher <jwbecher@drazisil.com> (drazisil.com)",
  "license": "MPL-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:drazisil/mco-server.git"
  },
  "engines": {
    "node": "<=v9.11.2"
  },
  "dependencies": {
    "@types/pg": "7.14.7",
    "@types/statsd-client": "0.4.1",
    "c-struct": "0.0.5",
    "debug": "4.3.1",
    "pg": "8.5.1",
    "pg-migrator": "1.1.0",
    "snyk": "1.436.0",
    "supports-color": "8.0.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0"
  },
  "devDependencies": {
    "@types/node": "14.14.13",
    "jsdoc": "3.6.6",
    "prettier-standard": "16.4.1",
    "supertest": "6.0.1",
    "tap": "14.11.0"
  },
  "snyk": true
}
